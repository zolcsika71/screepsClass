{
	"preset": "crockford",
        "maximumLineLength": 240,
        "requireCamelCaseOrUpperCaseIdentifiers": true,
        "validateIndentation": 4,
		"maxErrors": 1000,
        "requireCurlyBraces": [
          "while",
          "do",
          "try",
          "catch",
          "switch"
        ],
        "disallowKeywords": [""],
        "disallowDanglingUnderscores": { "allExcept":
        ["_structureMatrix", "_claimerMaxWeight", "_lastViewed", "_reservedSpawnEnergy", "_trav", "_strategyCache", "_ext"] }
}
